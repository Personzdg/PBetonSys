@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section scripts{
    <script src="~/Areas/Mms/ViewModels/Resource.js"></script>
    <script type="text/javascript">
        var data = @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(Model));
        ko.bindingViewModel(new viewModel(data));
    </script>
}
<div class="z-toolbar">
    <a href="#" plain="true" class="easyui-splitbutton" data-options="menu:'#dropdown',iconCls:'icon-download'">导出</a>
</div>
<div id="condition" class="container_12" style="position:relative;">
    <div class="grid_1 lbl">时间</div>
    <div class="grid_2 val"><input type="text" data-bind="value:form.IndateTime" class="z-txt easyui-daterange" style="width:150px;" /></div>
    <div class="grid_1 lbl">供应商名称</div>
    <div class="grid_2 val"><input type="text" data-bind="value:form.name" class="z-txt" /></div>
    <div class="grid_1 lbl">材料名称</div>
    <div class="grid_2 val"><input type="text" data-bind="value:form.TypeName" class="z-txt" /></div>
    <div class="clear"></div>
    <div class="prefix_9" style="position:absolute;top:5px;height:0;">
        <a id="btnSearch" href="#" plain="true" data-bind="click:searchClick" class="easyui-linkbutton" icon="icon-search" title="查询">查询</a>
    </div>
</div>



<div id="dropdown" style="width:100px; display:none;">
    <div data-options="iconCls:'icon-ext-xls'" suffix="xls" data-bind="click:downloadClick">Excel2003   </div>
    <div data-options="iconCls:'icon-page_excel'" suffix="xlsx" data-bind="click:downloadClick">Excel2007   </div>
    <div data-options="iconCls:'icon-ext-doc'" suffix="doc" data-bind="click:downloadClick">Word2003    </div>
</div>
<table id="gridlist" data-bind="datagrid:grid">
    <thead>
        <tr>
            <th field="_id" hidden="true">_id</th>
            <th field="id" width="80">入库单号</th>
            <th field="name" width="120">供应商名称</th>
            <th field="IndateTime" width="80">入库时间</th>
            <th field="TypeName" width="80">材料名称</th>
            <th field="Voiture_id" width="50">车号</th>
            <th field="silo_id" width="80">筒仓号</th>
            <th field="a_gross" width="50">毛重</th>
            <th field="a_empty" width="50">皮重</th>
            <th field="a_suttle" width="50">净重</th>
            <th field="a_suttleSJ" width="50">结算重量</th>
            <th field="a_price" width="50">单价</th>
            <th field="a_money" width="50">金额</th>
            <th field="FS" width="50">方数</th>
            <th field="FS_Ratio" width="50">方重折算率</th>
            <th field="FSPrice" width="50">方单价</th>
            <th field="FSMoney" width="50">方金额</th>
            <th field="a_unit" width="50">单位</th>
            <th field="MTType" width="50">类别</th>
            @*<th field="AffFlag" width="30">审核</th>*@
            <th field="BackDatetime" width="80">回皮时间</th>



        </tr>
    </thead>
</table>