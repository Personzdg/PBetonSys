@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section scripts{
    <script src="~/Areas/Mms/ViewModels/Receive.js"></script>
    <script type="text/javascript">
        var data = @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(Model));
        ko.bindingViewModel(new viewModel(data));
    </script>
}
<div class="z-toolbar">
    <a href="#" plain="true" class="easyui-splitbutton" data-options="menu:'#dropdown',iconCls:'icon-download'">导出</a>
</div>
<div id="condition" class="container_12" style="position:relative;">
    <div class="grid_1 lbl">时间</div>
    <div class="grid_2 val"><input type="text" data-bind="value:form.ctime" class="z-txt easyui-daterange" /></div>
    @*<div class="grid_1 lbl">工程名称</div>
        <div class="grid_1 val"><input type="text" data-bind="value:form.empid" class="z-txt easyui-combobox" data-options="showblank:true" /></div>*
    </div>*@
    <div class="prefix_9" style="position:absolute;top:5px;height:0;">
        <a id="btnSearch" href="#" plain="true" data-bind="click:searchClick" class="easyui-linkbutton" icon="icon-search" title="查询">查询</a>

    </div>


    <div id="dropdown" style="width:100px; display:none;">
        <div data-options="iconCls:'icon-ext-xls'" suffix="xls" data-bind="click:downloadClick">Excel2003   </div>
        <div data-options="iconCls:'icon-page_excel'" suffix="xlsx" data-bind="click:downloadClick">Excel2007   </div>
        <div data-options="iconCls:'icon-ext-doc'" suffix="doc" data-bind="click:downloadClick">Word2003    </div>
    </div>

    <table id="gridlist" data-bind="datagrid:grid">
        <thead>
            <tr>
                <th field="_id" hidden="true">_id</th>
                <th field="ctime" width="100">时间</th>
                <th field="bajarid" width="50">拌楼</th>
                <th field="empid" width="120">工程名称</th>
                <th field="stgid" width="50">强度</th>
                <th field="carserial" width="30">盘次</th>
                @*<th field="pno" width="80">出货单号</th>*@
                <th field="carid" width="50">车号</th>
                <th field="Mstring4" width="50">泵号</th>
                <th field="Mstring5" width="80">施工部位</th>
                <th field="qty" width="50">盘方量</th>
                <th field="num1" width="50">红5-25</th>
                <th field="num2" width="50">石5-16</th>
                <th field="num3" width="50">石5-25</th>
                <th field="num4" width="50">1#细砂</th>
                <th field="num5" width="50">2#粗砂</th>
                <th field="num6" width="50">水泥1</th>
                <th field="num7" width="50">水泥2</th>
                <th field="num8" width="50">矿粉</th>
                <th field="num9" width="50">粉煤灰</th>
                <th field="num10" width="50">水1</th>
                <th field="num11" width="50">水2</th>
                <th field="num12" width="50">外1</th>
                <th field="num13" width="50">外2</th>
                <th field="sumNum" width="50">合计</th>
            </tr>
        </thead>
    </table>
